<h1>Registered Applications</h1>

<div class="row">
  <div class="col-md-8">
    <% @applications.each do |app| %>
      <div class="media">
        <div class="media-body">
          <h4>
            <%= link_to app.name, user_registered_application_path(app.user, app) %>
          </h4>
        </div>
      </div>
    <% end %>
  </div>
</div>

<div class="row">
  <div class="col-md-6">
    <h2>Registered Applications</h2>
    <% @user.registered_applications.each do |app| %>
        <div class="row">
          <h5>
            <%= link_to app.name, user_registered_application_path(app.user, app) %>
          </h5>
      </div>
    <% end %>
  </div>
  <div class="row">
    <div class="col-md-12 text-center">
      <% if @user == current_user %>
        <%= link_to "Register another Application", new_user_registered_application_path(@user), class: 'btn btn-success' %>
      <% end %>
    </div>
  </div>
</div>

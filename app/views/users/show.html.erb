<div class="row">
  <div class="col-md-8">
    <h1>Hello <%= @user.name %></h1>
  </div>
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <h2>Registered Applications</h2>
      <% @user.registered_applications.each do |app| %>
        <div class="media">
          <div class="media-body">
            <h4>
              <%= link_to app.name, user_registered_application_path(app.user, app) %>
            </h4>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
